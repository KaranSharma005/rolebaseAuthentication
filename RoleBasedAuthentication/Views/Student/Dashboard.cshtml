@{
}

<div id="students-list">
</div>
<br />
<button type="button" id="addTeacherBtn" class="btn-primary">Add Student</button>

@section Scripts{
    <script>
        $(document).ready(function() {
            $.get("/Student/List", function(data, status) {
                if (status === "success") {
                    $("#students-list").html(data);
                }
            });
        });
    </script>
}